umn>\r\n\t\t\t\t<el-table-column label=\"状态\" prop=\"status\">\r\n\t\t\t\t\t<template slot-scope=\"scope\">\r\n\t\t\t\t\t\t<!-- {{scope.row}} 显示一列所有数据 -->\r\n\t\t\t\t\t\t<el-switch :active-value=\"1\" :inactive-value=\"0\" v-model=\"scope.row.status\" @change=\"changeStatus(scope.row)\">\r\n\t\t\t\t\t\t</el-switch>\r\n\t\t\t\t\t</template>\r\n\t\t\t\t</el-table-column>\r\n\t\t\t\t<el-table-column label=\"操作\" width=\"250px\">\r\n\t\t\t\t\t<template slot-scope=\"scope\">\r\n\t\t\t\t\t\t<!-- 修改 -->\r\n\t\t\t\t\t\t<el-button type=\"primary\" icon=\"el-icon-edit\" size=\"mini\" @click=\"showEditDialog(scope.row.id)\"></el-button>\r\n\t\t\t\t\t\t<!-- 删除 -->\r\n\t\t\t\t\t\t<el-button type=\"danger\" icon=\"el-icon-delete\" size=\"mini\" @click=\"deleteOpen(scope.row.id)\"></el-button>\r\n\t\t\t\t\t\t<!-- 分配角色 -->\r\n\t\t\t\t\t\t<el-tooltip effect=\"dark\" content=\"分配角色\" placement=\"top\" :enterable=\"false\">\r\n\t\t\t\t\t\t\t<el-button type=\"warning\" icon=\"el-icon-setting\" size=\"mini\"></el-button>\r\n\t\t\t\t\t\t</el-tooltip>\r\n\t\t\t\t\t</template>\r\n\t\t\t\t</el-table-column>\r\n\t\t\t</el-table>\r\n\t\t\t<el-pagination @size-change=\"handleSizeChange\" @current-change=\"handleCurrentChange\" :current-page=\"queryInfo.pageNum\"\r\n\t\t\t :page-sizes=\"[1, 2, 5, 10]\" :page-size=\"queryInfo.pageSize\" layout=\"total, sizes, prev, pager, next, jumper\" :total=\"total\">\r\n\t\t\t</el-pagination>\r\n\t\t</el-card>\r\n\r\n\r\n\t\t<!-- 添加用户Dialog标签 -->\r\n\t\t<el-dialog title=\"添加用户\" :visible.sync=\"addDialogVisible\" width=\"30%\" @close=\"addDialogClosed\">\r\n\t\t\t<!-- :before-close=\"handleClose\" -->\r\n\t\t\t<!-- 内容主题区 -->\r\n\t\t\t<el-form :model=\"addForm\" :rules=\"addFormRules\" ref=\"addFormRef\" label-width=\"100px\" class=\"demo-ruleForm\">\r\n\t\t\t\t<el-form-item label=\"用户名\" prop=\"username\">\r\n\t\t\t\t\t<!-- prop验证规则参数 -->\r\n\t\t\t\t\t<el-input v-model=\"addForm.username\"></el-input>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item label=\"昵称\" prop=\"nickname\">\r\n\t\t\t\t\t<!-- prop验证规则参数 -->\r\n\t\t\t\t\t<el-input v-model=\"addForm.nickname\"></el-input>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item label=\"密码\" prop=\"password\">\r\n\t\t\t\t\t<!-- prop验证规则参数 -->\r\n\t\t\t\t\t<el-input v-model=\"addForm.password\"></el-input>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item label=\"邮箱\" prop=\"email\">\r\n\t\t\t\t\t<!-- prop验证规则参数 -->\r\n\t\t\t\t\t<el-input v-model=\"addForm.email\"></el-input>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t</el-form>\r\n\t\t\t<!-- 底部区 -->\r\n\t\t\t<span slot=\"footer\" class=\"dialog-footer\">\r\n\t\t\t\t<el-button @click=\"addDialogVisible = false\">取 消</el-button>\r\n\t\t\t\t<el-button type=\"primary\" @click=\"addUser\">确 定</el-button>\r\n\t\t\t</span>\r\n\t\t</el-dialog>\r\n\r\n\t\t<!-- 修改用户Dialog标签 -->\r\n\t\t<el-dialog title=\"修改用户\" :visible.sync=\"editDialogVisible\" width=\"30%\" @close=\"editDialogClosed\">\r\n\t\t\t<!-- :before-close=\"handleClose\" -->\r\n\t\t\t<!-- 内容主题区 -->\r\n\t\t\t<el-form :model=\"editForm\" :rules=\"editFormRules\" ref=\"editFormRef\" label-width=\"100px\">\r\n\t\t\t\t<el-form-item label=\"用户名\" prop=\"username\">\r\n\t\t\t\t\t<!-- prop验证规则参数 -->\r\n\t\t\t\t\t<el-input v-model=\"editForm.username\" disabled></el-input>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item label=\"昵称\" prop=\"nickname\">\r\n\t\t\t\t\t<!-- prop验证规则参数 -->\r\n\t\t\t\t\t<el-input v-model=\"editForm.nickname\"></el-input>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item label=\"密码\" prop=\"password\">\r\n\t\t\t\t\t<!-- prop验证规则参数 -->\r\n\t\t\t\t\t<el-input v-model=\"editForm.password\"></el-input>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item label=\"邮箱\" prop=\"email\">\r\n\t\t\t\t\t<!-- prop验证规则参数 -->\r\n\t\t\t\t\t<el-input v-model=\"editForm.email\"></el-input>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t</el-form>\r\n\t\t\t<!-- 底部区 -->\r\n\t\t\t<span slot=\"footer\" class=\"dialog-footer\">\r\n\t\t\t\t<el-button @click=\"editDialogVisible = false\